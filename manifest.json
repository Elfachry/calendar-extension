{
    "manifest_version": 3,
    "name": "To do list",
    "version": "1.0",
    "description": "A simple to do list.",

    "permissions": [
        "tabs",
        "storage"
    ],

    "action": {
        "default_title": "Open To Do List"
    },

    "background": {
        "service_worker": "background.js"
    },

    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}